{
  "name": "rails-partial",
  "displayName": "Rails Partial",
  "version": "0.0.1",
  "publisher": "atipugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/atipugin/vscode-rails-partial.git"
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "engines": {
    "vscode": "^1.20.0"
  },
  "main": "./extension",
  "activationEvents": ["*"],
  "contributes": {
    "commands": [
      {
        "command": "rails-partial.createPartial",
        "title": "Rails Partial: Create partial"
      }
    ],
    "keybindings": [
      {
        "command": "rails-partial.createPartial",
        "key": "alt+p"
      }
    ]
  },
  "devDependencies": {
    "vscode": "^1.1.10"
  },
  "dependencies": {
    "mkdirp": "^0.5.1"
  }
}
